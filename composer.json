{
    "name": "syamsoul/laravel-action-delay",
    "description": "Delay a Job, Database Query or PHP code at a specific datetime easily with one simple command.",
    "type": "laravel-package",
    "keywords": [
        "syamsoul",
        "laravel",
        "action",
        "delay",
        "job",
        "database",
        "command",
        "artisan",
        "php",
        "souldoit"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SoulDoit\\ActionDelay\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Syamsoul Azrien",
            "email": "syamsoulazrien88@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "haydenpierce/class-finder": "^0.5.3",
        "illuminate/bus": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/queue": "^10.0|^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoulDoit\\ActionDelay\\Providers\\ActionDelayServiceProvider"
            ]
        }
    }
}
